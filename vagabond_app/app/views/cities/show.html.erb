<div>
<h2> <%= @city.try(:name) %></h2>
</div>

<div>
  <%= image_tag "sf.png" %>
</div>
<br>
<%= button_to "New City", new_city_path(@city), method: "get" %>
<% @city.posts.sort_by{|post| [post['updated_at']]}.reverse.each do |post| %>
<h3> <%= link_to post.title, city_post_path(@city, post) %></h3>
<% end %>
